
  <div class="container-fluid">
    <div class="row content">
      <div class="col-sm-2 sidenav">
        <form>
          <p> Apply Filter:</p>
          <p><input id="cafe" type="checkbox">cafe</input></p>
          <p><input id="library" type="checkbox">library</input></p>
          <p><input id="office Space" type="checkbox">office Space</input></p>
          <p><input type="submit" value="Apply"></input></p>
      </form>
      </div>
      <div class="col-sm-8 text-left">
        <p> Search for Workspaces:</p>
        <form>
          <div class="input-group">
            <input type="text" class="form-control" placeholder="Search">
            <div class="input-group-btn">
              <button class="btn btn-default" type="submit">
                <i class="glyphicon glyphicon-search"></i>
              </button>
            </div>
          </div>
        </form>
        
        
        

<!--//edit the jobs here-->
        <section>
          <h1>Workspaces</h1>
          {{#each workspace}}  
            <article class="workspace">
              <h2><a href="/info/workspaceDetails/{{_id}}">{{name}}</a></h2>
              <p>{{email}}</p>
              
            </article>
          {{/each}}
        </section>

      </div>
    </div>
  </div>

  





